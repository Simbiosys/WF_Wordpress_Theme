<html lang="es">
  <head>
  	<meta charset="utf-8">
  	<meta http-equiv="X-UA-Compatible" content="IE=edge">
  	<meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no,minimal-ui">
  	<meta name="apple-mobile-web-app-capable" content="yes">
  	<link rel="shortcut icon" type="image/x-icon" href="favicon.ico">
  	<link rel="apple-touch-icon" href="apple-touch-icon-precomposed.png">

  	<title data-localize="title">{{#labels}}{{title}}{{/labels}}</title>

  	<!-- Fonts -->
  	<link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,400,300,600,700,800|Open+Sans+Condensed:300,700|Merriweather:400,700,900' rel='stylesheet' type='text/css'>

  	<!-- Bootstrap -->
  	<link rel="stylesheet" href="{{path}}/styles/vendor/bootstrap.min.css">
  	<link rel="stylesheet" href="{{path}}/styles/screen.css">

  	<!-- addOns -->
  	<link rel="stylesheet" href="{{path}}/styles/vendor/owl.carousel-b2.css">
  	<link rel="stylesheet" href="{{path}}/styles/vendor/animate.css">

    <!-- Fonts -->
    <link rel="stylesheet" href="{{path}}/styles/vendor/custom-font.css">

    <!-- Datepicker -->
    <link rel="stylesheet" href="{{path}}/styles/vendor/bootstrap-datepicker3.min.css">

  	<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  	<!--[if lt IE 9]>
  	  <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
  	  <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
  	<![endif]-->

  	<!-- GOOGLE TRANSLATE-->
  	<meta name="google-translate-customization" content="d4011b835c1c0fff-9c22ee7e75da8a79-gadd50e162e5777df-10"></meta>

    <!-- Main jQuery  -->
    <script src="{{path}}/js/vendor/jquery-1.11.0.min.js"></script>

    <!-- Vendor and 3 parties jQuery  -->
    <script src="{{path}}/js/vendor/bootstrap.min.js"></script>
    <script src="{{path}}/js/vendor/owl.carousel.min-b2.js"></script>

    <!-- Utils -->
    <!-- 	<script src="js/plugins.js"></script> -->
    <script src="{{path}}/js/main.js"></script>

    <link href="//cdn-images.mailchimp.com/embedcode/classic-081711.css" rel="stylesheet" type="text/css">
  </head>

  <body>
  	<header>

  		<div class="bg-tools overfh clearb b-bottom hidden-xs">

  			<div class="container">

  				<div class="row">

  					<div class="col-lg-12">

  						<div class="displayib fleft t-spread-the-word p-xs-top">
  							<span class="lbl-spread displayib txt-xs c-g40">{{#labels}}{{spread_the_word}}{{/labels}}</span>
  							<a href="{{host}}/feed" class="displayib spread-block spread-rss">RSS</a>
  							<a href="https://www.facebook.com/webfoundation" class="displayib spread-block spread-facebook">facebook</a>
  							<a href="https://twitter.com/webfoundation" class="displayib spread-block spread-twitter">twitter</a>
  						</div>

  						<div class="displayib fright adj-mobile-tools">

  							<div class="t-translate-web displayib">
  								<div id="google_translate_element"></div>
                  <script type="text/javascript">
                    function googleTranslateElementInit() {
                      new google.translate.TranslateElement({pageLanguage: 'en', includedLanguages:
                    'en,es,ar,bn,zh-CN,fr,de,hi,ja,pt,ru,sw,it,sv,da,nl,ko,pl,ha,tr', layout: google.translate.TranslateElement.InlineLayout.SIMPLE, autoDisplay: false}, 'google_translate_element');
                    }
  								</script><script type="text/javascript" src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit"></script>
  							</div>

  							<form action="{{host}}" class="t-search-web displayib">
  								<input type="search" name="s" placeholder="{{#labels}}{{search_placeholder}}{{/labels}}" class="wf-input wf-input-search">
  							</form>
  						</div>

  					</div>

  				</div>

  			</div>

  		</div>


  		<div class="visible-xs visible-sm">

  			<nav class="navbar navbar-default">
  				<div class="container-fluid">
  					<div class="navbar-header">
  						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
  							<span class="sr-only">{{#labels}}{{toggle_navigation}}{{/labels}}</span>
  							<span class="icon-bar"></span>
  							<span class="icon-bar"></span>
  							<span class="icon-bar"></span>
  							<span class="navbar-lbl uppc txt-xs">{{#labels}}{{menu}}{{/labels}}</span>
  						</button>
  						<a class="navbar-brand" href="{{host}}"><img alt="Brand" src="{{path}}/img/wwwf-logo-mobile.png"></a>
  					</div>

  					<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
  						<form action="{{host}}" class="t-search-web b-bottom hidden-sm">
  							<input type="search" name="s" placeholder="Enter keyword, then press enter" class="wf-input wf-input-search">
  						</form>
              <div class="text-center">
                {{{navigation}}}
              </div>
  						<div class="area-donate txt-c"><a href="{{host}}/take-action" class="wf-btn wf-btn-action adj-btn-ac-top">{{#labels}}{{donate}}{{/labels}}</a></div>
  						<div class="displayb t-spread-the-word p-xs-top hidden-lg txt-c p-xs-bottom">
  							<div class="displayb overfh c-obj p-s-bottom">
  								<span class="lbl-spread displayib txt-xs c-g40">{{#labels}}{{spread_the_word}}{{/labels}}</span>
  								<a href="{{host}}/feed" class="displayib spread-block spread-rss">RSS</a>
  								<a href="https://www.facebook.com/webfoundation" class="displayib spread-block spread-facebook">facebook</a>
  								<a href="https://twitter.com/webfoundation" class="displayib spread-block spread-twitter">twitter</a>
  							</div>
  						</div>
  					</div>

  				</div>
  			</nav>

  		</div>


  		<div class="container hidden-xs hidden-sm">

  			<div class="row">

  				<div class="col-lg-12">

  					<div class="top-nav txt-c">

              <div class="top-logo displayib fleft hidden-md"><h1 class="no-m-bottom no-m-top"><a href="{{host}}" class="txt-ind displayb top-logo-h n-outl">World Wide Web Foundation</a></h1></div>
						  <div class="top-logo displayib fleft visible-md"><h1 class="no-m-bottom no-m-top"><a href="{{host}}" class="txt-ind displayb top-logo-h-mobile n-outl">World Wide Web Foundation</a></h1></div>

              <div class="area-donate fright">
  							<a href="{{host}}/take-action" class="wf-btn wf-btn-action adj-btn-ac-top">{{#labels}}{{donate}}{{/labels}}</a>
  						</div>

  						<nav class="displayib c-obj">
                {{{navigation}}}
  						</nav>

  					</div>

  				</div><!--end col-->

  			</div><!--end row-->

  		</div><!--end container-->


    </header>
    <!--end main header-->
