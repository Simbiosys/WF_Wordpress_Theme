  	<title data-localize="title">'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['title']) && is_array($in)) ? $in['title'] : null), ENT_QUOTES, 'UTF-8').'';}).'</title>

  	<!-- Fonts -->
