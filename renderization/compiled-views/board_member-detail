			<h4 class="m-s-bottom m-xs-top">'.htmlentities((string)((isset($in['data']['person']['position']) && is_array($in['data']['person'])) ? $in['data']['person']['position'] : null), ENT_QUOTES, 'UTF-8').'</h4>
