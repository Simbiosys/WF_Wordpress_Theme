					<option value="'.htmlentities((string)((isset($in['url']) && is_array($in)) ? $in['url'] : null), ENT_QUOTES, 'UTF-8').'" class="snav-go">'.htmlentities((string)((isset($in['title']) && is_array($in)) ? $in['title'] : null), ENT_QUOTES, 'UTF-8').'</option>
