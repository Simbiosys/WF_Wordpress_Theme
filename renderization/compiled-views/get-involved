<?php return function ($in, $debugopt = 1) {
    $cx = array(
        'flags' => array(
            'jstrue' => false,
            'jsobj' => false,
            'spvar' => false,
            'prop' => false,
            'method' => false,
            'mustlok' => false,
            'echo' => false,
            'debug' => $debugopt,
        ),
        'constants' =>  array(
            'DEBUG_ERROR_LOG' => 1,
            'DEBUG_ERROR_EXCEPTION' => 2,
            'DEBUG_TAGS' => 4,
            'DEBUG_TAGS_ANSI' => 12,
            'DEBUG_TAGS_HTML' => 20,
        ),
        'helpers' => array(),
        'blockhelpers' => array(),
        'hbhelpers' => array(),
        'partials' => array(),
        'scopes' => array(),
        'sp_vars' => array('root' => $in),
        'lcrun' => 'LCRun3',
'funcs' => array(
    'sec' => function ($cx, $v, $in, $each, $cb, $else = null) {
        $isAry = is_array($v);
        $isTrav = $v instanceof Traversable;
        $loop = $each;
        $keys = null;
        $last = null;
        $isObj = false;

        if ($isAry && $else !== null && count($v) === 0) {
            $cx['scopes'][] = $in;
            $ret = $else($cx, $in);
            array_pop($cx['scopes']);
            return $ret;
        }

        // #var, detect input type is object or not
        if (!$loop && $isAry) {
            $keys = array_keys($v);
            $loop = (count(array_diff_key($v, array_keys($keys))) == 0);
            $isObj = !$loop;
        }

        if (($loop && $isAry) || $isTrav) {
            if ($each && !$isTrav) {
                // Detect input type is object or not when never done once
                if ($keys == null) {
                    $keys = array_keys($v);
                    $isObj = (count(array_diff_key($v, array_keys($keys))) > 0);
                }
            }
            $ret = array();
            $cx['scopes'][] = $in;
            $i = 0;
            if ($cx['flags']['spvar']) {
                $old_spvar = $cx['sp_vars'];
                $cx['sp_vars'] = array(
                    '_parent' => $old_spvar,
                    'root' => $old_spvar['root'],
                );
                if (!$isTrav) {
                    $last = count($keys) - 1;
                }
            }
            foreach ($v as $index => $raw) {
                if ($cx['flags']['spvar']) {
                    $cx['sp_vars']['first'] = ($i === 0);
                    $cx['sp_vars']['last'] = ($i == $last);
                    $cx['sp_vars']['key'] = $index;
                    $cx['sp_vars']['index'] = $i;
                    $i++;
                }
                $ret[] = $cb($cx, $raw);
            }
            if ($cx['flags']['spvar']) {
                if ($isObj) {
                    unset($cx['sp_vars']['key']);
                } else {
                    unset($cx['sp_vars']['last']);
                }
                unset($cx['sp_vars']['index']);
                unset($cx['sp_vars']['first']);
                $cx['sp_vars'] = $old_spvar;
            }
            array_pop($cx['scopes']);
            return join('', $ret);
        }
        if ($each) {
            if ($else !== null) {
                $cx['scopes'][] = $in;
                $ret = $else($cx, $v);
                array_pop($cx['scopes']);
                return $ret;
            }
            return '';
        }
        if ($isAry) {
            $cx['scopes'][] = $in;
            $ret = $cb($cx, $v);
            array_pop($cx['scopes']);
            return $ret;
        }

        if ($v === true) {
            return $cb($cx, $in);
        }

        if (!is_null($v) && ($v !== false)) {
            return $cb($cx, $v);
        }

        if ($else !== null) {
            $cx['scopes'][] = $in;
            $ret = $else($cx, $in);
            array_pop($cx['scopes']);
            return $ret;
        }

        return '';
    },
)

    );
    
    return '	<!-- INTRO BLOCK -->
	<div class="main-intro-section mintro-section-c-redb txt-c overfh m-xl-bottom">
		<div class="intro-section displayb c-obj c-contrast overfh p-xxl-top p-xxl-bottom">
			<h3 class="no-m-top m-xs-bottom fbold">'.htmlentities((string)((isset($in['data']['support-us']['content']['title']) && is_array($in['data']['support-us']['content'])) ? $in['data']['support-us']['content']['title'] : null), ENT_QUOTES, 'UTF-8').'</h3>
			<h4 class="m-s-bottom m-xs-top">'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['support_us_breadcrumb']) && is_array($in)) ? $in['support_us_breadcrumb'] : null), ENT_QUOTES, 'UTF-8').'';}).'</h4>
			<div class="c-s-separator-intro c-obj"></div>
		</div>
	</div>

	<!-- SUBNAVS -->
	<div class="global-content clearb overfh">

		<div class="sunav-left hidden-sm hidden-xs">

			<div class="c-subnav">

				'.((isset($in['navigation-support-us']) && is_array($in)) ? $in['navigation-support-us'] : null).'

			</div><!--end c-subnav-->

		</div>

		'.'<div class="social-nav hidden-sm hidden-xs">
  <div class="c-social">
    <nav>
      <ul class="social-items pos-snav">
        '.'<li class="p-xs-bottom"><span class="social-title txt-xs c-g40">'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['spread_the_word']) && is_array($in)) ? $in['spread_the_word'] : null), ENT_QUOTES, 'UTF-8').'';}).'</span></li>
<li class="item-s txt-s"><span class="icon-twitter icon-s-twitter"></span> <a class="item-s-a" href="http://twitter.com/share?url='.htmlentities((string)((isset($in['url']) && is_array($in)) ? $in['url'] : null), ENT_QUOTES, 'UTF-8').'&via=webfoundation">Twitter</a></li>
<li class="item-s txt-s"><span class="icon-facebook icon-s-facebook"></span> <a class="item-s-a" href="http://www.facebook.com/sharer.php?u='.htmlentities((string)((isset($in['url']) && is_array($in)) ? $in['url'] : null), ENT_QUOTES, 'UTF-8').'">Facebook</a></li>
<li class="item-s txt-s"><span class="icon-gplus icon-s-gplus"></span> <a class="item-s-a" href="https://plus.google.com/share?url='.htmlentities((string)((isset($in['url']) && is_array($in)) ? $in['url'] : null), ENT_QUOTES, 'UTF-8').'">Google+</a></li>
'.'
      </ul>
    </nav>
  </div><!--end c-social-->
</div>
'.'

		<!--CONTENT BLOCK-->
		<div class="container-max-height">

		<div class="container-fluid pos-content p-xxl-bottom main-content-st">


			<div class="row m-l-bottom">
				<div class="col-lg-12 col-md-12 col-sm-12">
					<nav>
						<ul class="n-breadscrumb">
							<li class="item-bs displayb"><a href="'.htmlentities((string)((isset($in['host']) && is_array($in)) ? $in['host'] : null), ENT_QUOTES, 'UTF-8').'" class="item-a-bs avisibility">'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['home']) && is_array($in)) ? $in['home'] : null), ENT_QUOTES, 'UTF-8').'';}).'</a></li>
							<li class="item-bs displayb"><span class="item-a-bs item-a-bs-active">'.htmlentities((string)((isset($in['data']['support-us']['content']['title']) && is_array($in['data']['support-us']['content'])) ? $in['data']['support-us']['content']['title'] : null), ENT_QUOTES, 'UTF-8').'</span></li>
						</ul>
					</nav>
				</div>
			</div>
			<!--end Breadcrumb-->

			<div class="row">

				<div class="col-lg-12 col-md-12 col-sm-12">

					<div class="get-involved-hero">
						<div class="bg-get-involved-hero" style="background-image: url('.htmlentities((string)((isset($in['data']['support-us']['content']['image']) && is_array($in['data']['support-us']['content'])) ? $in['data']['support-us']['content']['image'] : null), ENT_QUOTES, 'UTF-8').');">
							<div class="get-i-content c-contrast">
								<h3 class="no-m-top fbold">'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['support_us']) && is_array($in)) ? $in['support_us'] : null), ENT_QUOTES, 'UTF-8').'';}).'</h3>
								<p class="f-merryw highlighted">'.((isset($in['data']['support-us']['content']['highlighted']) && is_array($in['data']['support-us']['content'])) ? $in['data']['support-us']['content']['highlighted'] : null).'</p>
							</div>
						</div>
					</div>
					<p class="text-right m-right c-g40 txt-s">'.((isset($in['data']['support-us']['content']['credits']) && is_array($in['data']['support-us']['content'])) ? $in['data']['support-us']['content']['credits'] : null).'</p>

				</div>

			</div><!--end row-->

			<div class="row m-l-top">
				<div class="col-xs-12 post-container">
					'.((isset($in['data']['support-us']['content']['content']) && is_array($in['data']['support-us']['content'])) ? $in['data']['support-us']['content']['content'] : null).'
				</div>
			</div>

			<div class="row m-l-top">

				<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">

					<h4>'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['join_the']) && is_array($in)) ? $in['join_the'] : null), ENT_QUOTES, 'UTF-8').'';}).' <span class="uppc fbold">'.$cx['funcs']['sec']($cx, ((isset($in['labels']) && is_array($in)) ? $in['labels'] : null), $in, false, function($cx, $in) {return ''.htmlentities((string)((isset($in['conversation']) && is_array($in)) ? $in['conversation'] : null), ENT_QUOTES, 'UTF-8').'';}).'</span></h4>
					<div class="adj-content-jcon c-obj p-s-top overfh">
						<a href="#cfacebook" class="j-conv jc-facebook clearb fleft"><span class="j-tit">On Facebook</span></a>
						<a href="#ctwitter" class="j-conv jc-twitter clearb fright"><span class="j-tit txt-rg">On Twitter</span></a>
