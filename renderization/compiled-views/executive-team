							<div class="gird-dk visible-lg">

								'.(($cx['funcs']['ifvar']($cx, ((isset($in['data']['team-1']['0']) && is_array($in['data']['team-1'])) ? $in['data']['team-1']['0'] : null))) ? '
								<div class="b-team b-team-large fleft" style="background-image: url('.htmlentities((string)((isset($in['data']['team-1']['0']['image']) && is_array($in['data']['team-1']['0'])) ? $in['data']['team-1']['0']['image'] : null), ENT_QUOTES, 'UTF-8').');">
